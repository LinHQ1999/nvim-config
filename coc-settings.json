{
  "languageserver": {
    "clangd": {
      "command": "clangd",
      "args": ["--background-index"],
      "rootPatterns": [
        "compile_flags.txt",
        "compile_commands.json",
        ".clang-format",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    }
  },
  "Lua.completion.enable": false,
  "Lua.diagnostics.globals": ["vim"],
  "coc.preferences.formatOnType": false,
  "diagnostic.enableMessage": "always",
  "diagnostic.errorSign": "E",
  "diagnostic.hintSign": "H",
  "diagnostic.infoSign": "I",
  "diagnostic.virtualText": false,
  "diagnostic.virtualTextLineSeparator": " ﬋ ",
  "diagnostic.virtualTextPrefix": "  ",
  "diagnostic.warningSign": "W",
  "eslint.enable": true,
  "html.filetypes": ["html", "vue", "handlebars", "template", "jsx"],
  "html.format.indentHandlebars": true,
  "html.format.wrapAttributes": "force-aligned",
  "javascript.format.semicolons": "ignore",
  "javascript.suggest.autoImports": true,
  "javascript.suggest.completeJSDocs": true,
  "rust-analyzer.checkOnSave.enable": false,
  "rust-analyzer.server.path": "D:\\Binaries\\rust-analyzer.exe",
  "semanticTokens.enable": true,
  "snippets.ultisnips.directories": ["UltiSnips", "gosnippets/UltiSnips"],
  "snippets.ultisnips.enable": true,
  "suggest.enablePreselect": false,
  "suggest.noselect": true,
  "suggest.virtualText": true,
  "typescript.preferences.jsxAttributeCompletionStyle": "auto",
  "volar.autoClosingTags": true,
  "volar.autoCompleteRefs": true,
  "volar.diagnostics.tsLocale": "zh-cn"
}
