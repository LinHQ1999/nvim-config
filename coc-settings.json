{
    "languageserver": {
        "clangd": {
            "command": "clangd",
            "args": [
                "--background-index"
            ],
            "rootPatterns": [
                "compile_flags.txt",
                "compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "filetypes": [
                "c",
                "cpp",
                "objc",
                "objcpp"
            ]
        },
        "kotlin": {
            "command": "F:\\KotlinLS\\server\\bin\\kotlin-language-server.bat",
            "filetypes": [
                "kotlin"
            ]
        }
    },
    "suggest.enablePreview": true,
    "suggest.noselect": true,
    "java.configuration.updateBuildConfiguration": "interactive",
    "java.import.maven.enabled": true,
    "java.errors.incompleteClasspath.severity": "ignore",
    "java.jdt.ls.home": "F:/JDT",
    "java.configuration.maven.userSettings": "C:/Users/peruser/.m2/settings.xml"
}
