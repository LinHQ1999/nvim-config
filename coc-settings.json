{
    "languageserver": {
        "clangd": {
            "command": "clangd",
            "args": [
                "--background-index"
            ],
            "rootPatterns": [
                "compile_flags.txt",
                "compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "filetypes": [
                "c",
                "cpp",
                "objc",
                "objcpp"
            ]
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": [
                "go.mod",
                ".git/"
            ],
            "disableWorkspaceFolders": true,
            "filetypes": [
                "go"
            ],
            "initializationOptions": {
                "usePlaceholders": true
            }
        },
//        "omnisharp": {
//            "command": "F:\\Roslyn\\2020-04-01-17-32-17-download-omnisharp-win-x64\\OmniSharp.exe",
//            "args": [
//                "-lsp"
//            ],
//            "rootPatterns": [
//                ".git/",
//                ".csproj"
//            ],
//            "filetypes": [
//                "cs"
//            ],
//            "settings": {}
//        }
    },
    "suggest.enablePreview": true,
    "java.errors.incompleteClasspath.severity": "ignore",
    "suggest.noselect": true,
    "java.configuration.updateBuildConfiguration": "interactive",
    "java.jdt.ls.home": "F:/JDT/lang",
    "eslint.enable": true
}
